  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
